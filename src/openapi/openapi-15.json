            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MerchantResponseEntity"
                }
              }
            }
          },
          "401": {
            "description": "Invalid Partner API Key",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationError"
                }
              }
