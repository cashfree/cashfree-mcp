{
  "openapi": "3.0.0",
  "info": {
    "version": "2023-01-01",
    "title": "Merchant Onboarding APIs"
  },
  "servers": [
    {
      "url": "https://api-sandbox.cashfree.com/partners",
      "description": "Sandbox server"
