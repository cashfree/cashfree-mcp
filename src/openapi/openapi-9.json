    "/payout/v1/validation/upiDetails": {
      "get": {
        "tags": [
          "Bank Account Verification"
        ],
        "summary": "UPI Verification",
        "description": "Use this API to verify if a given UPI handle exists. You will receive the customer name given at the bank in the response for valid UPIs.",
        "operationId": "upiVerification",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "description": "Bearer Auth Token",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Content-Type",
            "in": "header",
