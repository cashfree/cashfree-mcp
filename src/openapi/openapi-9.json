                      "properties": {
                        "nameAtBank": {
                          "type": "string",
                          "description": "It represents the name of the account holder as per the bank registration.",
                          "example": "JOHN DOE"
                        },
                        "accountExists": {
                          "type": "string",
                          "description": "It represents the existence information of the account.",
                          "example": "YES"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
